{
  "rewrites": [
    {
      "source": "/Form",
      "destination": "/index.html"
    },
    {
      "source": "/Form/",
      "destination": "/index.html"
    },
    {
      "source": "/Form/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/Records",
      "destination": "/index.html"
    },
    {
      "source": "/Records/",
      "destination": "/index.html"
    },
    {
      "source": "/Records/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}